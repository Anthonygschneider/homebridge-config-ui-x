{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES2022",
    "moduleResolution":"node",
    "outDir": "./dist",
    "rootDir": "./",
    "sourceMap": true,
    "target": "ES2022"
  },
  "exclude": [
    "node_modules",
    "./dist",
    "ui",
    "public"
  ]
}
